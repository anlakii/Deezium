{

  "description": "Upgrades your Deezer account to premium",
  "manifest_version": 2,
  "name": "Deezium",
  "version": "1.1",
  "homepage_url": "https://github.com/lakiluki1",
  "permissions": [
    "webRequest", "fetch", "webRequestBlocking", "https://e-cdns-files.dzcdn.net/cache/js/*", "https://www.deezer.com/*", "https://api.deezer.com/*"
  ],

  "background": {
    "scripts": ["background.js", "aes.js"]
  },

  "applications": {
    "gecko": {
      "strict_min_version": "70.00"
    }
  }

}
